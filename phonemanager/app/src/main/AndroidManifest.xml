<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feicui.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- 打电话权限 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 杀进程权限 -->
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <!-- 相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- sd卡的读写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".LeadActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogoActivity" >
        </activity>
        <activity
            android:name=".AboutUsActivity"
            android:label="@string/title_activity_about_us" >
        </activity>
        <activity
            android:name=".SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name=".TelManagerActivity"
            android:label="@string/title_activity_tel_manager" >
        </activity>
        <activity
            android:name=".TelListActivity"
            android:label="@string/title_activity_tel_list" >
        </activity>
        <activity
            android:name=".SpeedUpActivity"
            android:label="@string/title_activity_speed_up" >
        </activity>
        <activity
            android:name=".SoftManagerActivity"
            android:label="@string/title_activity_soft_manager" >
        </activity>
        <activity
            android:name=".SoftListActivity"
            android:label="@string/title_activity_soft_list" >
        </activity>
        <activity
            android:name=".PhoneCheckActivity"
            android:label="@string/title_activity_phone_check" >
        </activity>
        <activity
            android:name=".FileManagerActivity"
            android:label="@string/title_activity_file_manager" >
        </activity>
        <activity
            android:name=".FileListActivity"
            android:label="@string/title_activity_file_list" >
        </activity>
        <activity
            android:name=".ClearActivity"
            android:label="@string/title_activity_clear" >
        </activity>
    </application>

</manifest>